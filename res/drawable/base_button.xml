<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#B75767" />
      <stroke
          android:width="1dp"
          android:color="#B75767" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#FA9B4D"
          android:endColor="#FA9B4D"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#FA9B4D" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>
